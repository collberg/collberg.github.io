        {
          "_customTemplate": true,
          "_afterNavbarItem": false,
          "_customHTML": "<!-- Type valid HTML here -->\n<center>\n<pre style=\"text-align:left;background-color:#f0f0f0;display:inline-block;padding:20px;border:1px solid black\"><code>$ tigress-merge file1.c file2.c --out=full.c\n</code></pre></center>",
          "_customCSS": "/* Type valid CSS here */\n\ndiv {\n  padding: 80px 0;\n  color: #777;\n  text-align: center;\n}\np {\n  font-size: 60px;\n  color: #777;\n}",
